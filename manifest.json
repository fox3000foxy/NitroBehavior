{
    "name": "Nitro Behaviour",
    "description": "Stimule un nitro sur l'interface web de discord",
    "version": "2.3.1",
    "permissions": [
        "activeTab",
        "tabs",
        "storage",
        "scripting"
    ],
    "host_permissions": [
        "*://*/*"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "web_accessible_resources": [{
        "resources": [
            "background-remote.js",
            "nitro.js"
        ],
        "matches": [
            "<all_urls>"
        ]
    }],
    "action": {
        "default_title": "",
        "default_icon": "icon.png"
    },
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "manifest_version": 3
}